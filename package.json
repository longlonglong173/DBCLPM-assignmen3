  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn lint"
    ],
    "src/**/*.{json,css,scss,sass}": [
      "yarn format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "npm": "please use yarn",
    "yarn": ">= 1.19.1"
  },
  "scripts": {
    "start": "HTTPS=true react-app-rewired start",
    "start:dev": "HTTPS=true env-cmd -f .env.dev react-app-rewired start",
    "start:stg": "HTTPS=true env-cmd -f .env.stg react-app-rewired start",
    "start:prod": "HTTPS=true env-cmd -f .env.prod react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "env-cmd -f .env.dev react-app-rewired build",
    "build:stg": "env-cmd -f .env.stg react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "test:dev": "env-cmd -f .env.dev react-app-rewired test",
    "test:stg": "env-cmd -f .env.stg react-app-rewired test",
    "test:prod": "env-cmd -f .env.prod react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier src/**/*.{ts,tsx,json} --write",
    "lint": "eslint src/**/*.{ts,tsx} --fix --quiet"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@types/jest": "^26.0.14",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/lodash.snakecase": "^4.1.6",
    "@types/node": "^14.11.2",
    "@types/obj-str": "^1.0.0",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-router-dom": "^5.1.5",
    "@types/redux-persist": "^4.3.1",
    "@types/webpack-env": "^1.16.0",
    "@types/yup": "^0.29.8",
    "commitlint-config-jira": "^1.4.1",
    "commitlint-plugin-jira-rules": "^1.4.0",
    "customize-cra": "^1.0.0",
    "customize-cra-react-refresh": "^1.1.0",
    "env-cmd": "^10.1.0",
    "eslint-config-airbnb-typescript-prettier": "^3.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "typescript": "3.9.7"
  }
